varnishtest "unreferenced or null acls"

varnish v1 -arg "-p vcc_err_unref=off" -vcl {
	import vtc;
	backend s1 {
		.host = "${bad_backend}";
	}
	acl foo {
		"${localhost}";
	}
	acl bar {
		"${localhost}";
	}
	sub vcl_recv {
		if (vtc.no_ip() ~ bar) {
			return (synth(200));
		}
	}
} -start

client c1 {
	txreq
	rxresp
	expect resp.status == 503
	expect resp.body ~ "VCL failed"
} -run
