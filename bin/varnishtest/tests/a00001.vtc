# $Id$

test "basic default HTTP transactions with expect"

server s1 {
	rxreq
	expect req.request == GET
	expect req.proto == HTTP/1.1
	expect req.url == "/"
	txresp 
}

server s1 -start 

client c1 -connect $s1_sock {
	txreq 
	rxresp
	expect resp.proto == HTTP/1.1
	expect resp.status == 200
	expect resp.msg == Ok
}

client c1 -run

server s1 -wait
