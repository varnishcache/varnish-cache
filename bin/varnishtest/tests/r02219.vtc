varnishtest "#2219: PROXY tight workspace handling"

server s1 {
	rxreq
	txresp
	rxreq
	txresp
	rxreq
	txresp
} -start

varnish v1 -arg "-p workspace_client=9k" -proto PROXY -vcl+backend {
	sub vcl_recv {
		return (pass);
	}
} -start

client c1 {
	send "PROXY TCP4 127.0.0.1 127.0.0.1 1111 2222\r\nGET  HTTP/1.1\r\n\r\n"
	rxresp
} -run

