varnishtest "Ignoring non esi elements"

server s1 {
	rxreq 
	txresp -body {
		<f<esi?
		<esi:remove>
			bar
		</esi:remove>
		foo
	}
} -start

varnish v1 -vcl+backend {
	sub vcl_fetch {
		set beresp.do_esi = true;
	}
} -start -cli "param.set esi_syntax 2"

client c1 {
	txreq 
	rxresp
	expect resp.status == 200
	expect resp.bodylen == 21
} -run
varnish v1 -expect esi_errors == 0
