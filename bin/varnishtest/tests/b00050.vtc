varnishtest "VXID log filtering"

server s1 {
	rxreq
	txresp
} -start

varnish v1 -arg "-p thread_pools=1" -vcl+backend { } -start

logexpect l1 -v v1 -q "vxid == 1001" {
	expect 0 1001 Begin "req 1000 rxreq"
} -start

client c1 {
	txreq
	rxresp
} -run

logexpect l1 -wait

# vxid only supports integer operations

err_shell "Expected vxid operator got '~'" {
	varnishlog -n ${v1_name} -d -q 'vxid ~ 1001' 2>&1
}

err_shell "Expected vxid operator got '!~'" {
	varnishlog -n ${v1_name} -d -q 'vxid !~ 1001' 2>&1
}

err_shell "Expected vxid operator got 'eq'" {
	varnishlog -n ${v1_name} -d -q 'vxid eq 1001' 2>&1
}

err_shell "Expected vxid operator got 'ne'" {
	varnishlog -n ${v1_name} -d -q 'vxid ne 1001' 2>&1
}

# vxid only supports integer operands

err_shell "Expected integer got '1001.5'" {
	varnishlog -n ${v1_name} -d -q 'vxid != 1001.5' 2>&1
}

# vxid doesn't support taglist selection

err_shell "Unexpected taglist selection for vxid" {
	varnishlog -n ${v1_name} -d -q 'vxid[1] >= 1001' 2>&1
}

err_shell "Unexpected taglist selection for vxid" {
	varnishlog -n ${v1_name} -d -q '{1}vxid <= 1001' 2>&1
}

err_shell "Unexpected taglist selection for vxid" {
	varnishlog -n ${v1_name} -d -q 'vxid,Link > 1001' 2>&1
}

err_shell "Unexpected taglist selection for vxid" {
	varnishlog -n ${v1_name} -d -q 'vxid,vxid < 1001' 2>&1
}
