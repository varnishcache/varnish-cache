varnishtest "Unformattable VCL_TIME"

builtwith 64bit

server s1 {
	rxreq
	txresp
} -start

varnish v1 -vcl+backend {
	import std;

	sub vcl_deliver {
		set resp.http.ts = std.time(
		    real=std.real("999999999999.999", 0) * std.real("999999999999.999", 0),
		    fallback=now);
	}
} -start

client c1 {
	txreq
	rxresp
	expect resp.status == 503
} -run
