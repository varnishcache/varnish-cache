# $Id$

test "ESI test with no ESI content"


server s1 {
	rxreq 
	txresp -body {
		-This is a test: Hello world
	}
} -start

varnish v1 -vcl+backend {
	sub vcl_fetch {
		set beresp.do_esi = true;
	}
} -start

client c1 {
	txreq 
	rxresp
	expect resp.status == 200
	expect resp.http.content-length == 33
}

client c1 -run
varnish v1 -expect esi_errors == 0
