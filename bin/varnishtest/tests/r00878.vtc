varnishtest "Loading vmods in subsequent VCLs"

server s1 {
	rxreq
	txresp -bodylen 4
} -start

varnish v1 -vcl+backend {
        import ${vmod_debug};
	sub vcl_deliver {
		set resp.http.who = debug.author(phk);
	}
} -start


client c1 {
	txreq
	rxresp
} -run
varnish v1 -vcl+backend {
        import ${vmod_debug};
	sub vcl_deliver {
		set resp.http.who = debug.author(des);
	}
}

client c1 {
	txreq
	rxresp
} -run

varnish v1 -vcl+backend {
        import ${vmod_debug};
	sub vcl_deliver {
		set resp.http.who = debug.author(kristian);
	}
}

client c1 {
	txreq
	rxresp
} -run
